{
  "version": 2,
  "builds": [
    {
      "src": "/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.ts" },
    {"src": "/page/(.*)", "dest": "/"}
  ]
}
